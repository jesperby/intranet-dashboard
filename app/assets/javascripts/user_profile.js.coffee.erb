jQuery ->
  tokenInputOptions = {
    theme: 'malmo',
    searchDelay: 0,
    minChars: 2,
    animateDropdown: false,
  }

  $('#user_language_list').tokenInput(
    '<%= asset_path("/languages/search.json") %>', $.extend({
      prePopulate: $('#user_language_list').data('load'),
      hintText: "Lägg till språk"
    }, tokenInputOptions)
  )