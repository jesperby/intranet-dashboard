= form_tag(group_contacts_search_path, method: "get", id: "search-group-contacts", class: "form-horizontal", "data-path" => group_contacts_path) do
  .control-group
    = label_tag "q", "Sök funktionskontakt: ", class: "control-label"
    .controls
      .input-append
        = text_field_tag "q", params[:q]
        = submit_tag "Sök", { class: "btn", name: nil }
