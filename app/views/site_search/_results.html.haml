    %p.extract
      = entry.summary
      %span.date= entry.date
      - # unless entry.content_type == "html"
      - #   %span.file_info= "(#{entry.content_type} #{entry.file_size})"

    %p.category= entry.category
    - # %ul.breadcrumb= entry.breadcrumb

- if @results.more_query.present?
  %li#load-more-search-results= link_to "Visa fler tr√§ffar", "#{search_path}?#{@results.more_query}", class: "btn"
